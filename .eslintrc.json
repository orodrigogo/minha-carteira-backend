{
  "env": {
      "commonjs": true,
      "es2020": true,
      "node": true
  },
  "extends": [
      "airbnb-base",
      "prettier"
  ],
  "plugins": [
      "prettier"
  ],
  "parserOptions": {
      "ecmaVersion": 11
  },
  "rules": {
      "prettier/prettier": ["error", {"endOfLine":"auto"}]
  }
}
